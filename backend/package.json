{
  "name": "lang-chain-project",
  "version": "1.0.0",
  "description": "A TypeScript project using LangChain with React chat interface",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "nodemon --exec ts-node src/index.ts",
    "watch": "tsc --watch",
    "test:chatwoot": "node test-chatwoot.js"
  },
  "dependencies": {
    "@figuro/chatwoot-sdk": "^1.6.2",
    "@langchain/core": "^0.3.62",
